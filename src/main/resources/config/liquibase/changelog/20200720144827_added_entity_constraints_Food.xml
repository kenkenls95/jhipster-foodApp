<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.9.xsd">
    <!--
        Added the constraints for entity Food.
    -->
    <changeSet id="20200720144827-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="billdetail_id"
                                 baseTableName="food"
                                 constraintName="fk_food_billdetail_id"
                                 referencedColumnNames="id"
                                 referencedTableName="billdetail"/>

        <addForeignKeyConstraint baseColumnNames="food_id"
                                 baseTableName="food_category"
                                 constraintName="fk_food_category_food_id"
                                 referencedColumnNames="id"
                                 referencedTableName="food"/>

        <addForeignKeyConstraint baseColumnNames="category_id"
                                 baseTableName="food_category"
                                 constraintName="fk_food_category_category_id"
                                 referencedColumnNames="id"
                                 referencedTableName="category"/>
        
    </changeSet>
</databaseChangeLog>
